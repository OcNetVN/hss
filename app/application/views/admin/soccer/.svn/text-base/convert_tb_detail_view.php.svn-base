<--main content start-->
  <section id="main-content">
      <section class="wrapper site-min-height">
          <!-- page start-->
          
          <!-- table start -->
          <div class="row">                  
              <div class="col-lg-12">
                <form class="form-horizontal" role="form">
                  <div class="panel">
                    <div class="panel-body">
                      <div class="form-group">
                      <div class="panel">
                      <div class="row">
                       <div class="col-md-12">
                            <div class="col-md-3">
                                <label>List country:</label>
                                <select  class="form-control" name="lstCountry" id="lstCountry" >
                                </select> 
                            </div>
                            <div class="col-md-3">
                                  <label>League:</label>
                                  <select  class="form-control" name="lstLeague" id="lstLeague" >
                                  </select> 
                            </div>
                            <div class="col-md-3">
                                <label>Venue:</label>
                                <select  class="form-control" name="lstVenue" id="lstVenue">
                                    <option value="All">All</option>
                                    <option value="Home">Home</option>
                                    <option value="Away">Season</option>
                                </select> 
                            </div>
                            <div class="col-md-3">
                              <label>Season:</label>
                              <select  class="form-control" name="lstSeason" id="lstSeason">
                                    <option value="201516">2015/2016</option>
                                    <option value="201415">2014/2015</option>
                                    <option value="201314">2013/2014</option>
                                    <option value="201413">2014/2013</option>
                                    <option value="201312">2013/2012</option>
                                    <option value="201211">2012/2011</option>
                                    <option value="201110">2011/2010</option>
                              </select> 
                            </div>
                         <br />   
                       </div>
                    </div>
                    <br/>
                    <div class="row">
                       <div class="col-md-12">
                          <div class="col-md-3"> </div>
                          <div class="col-md-4">
                              <div data-date-format="dd-mm-yyyy" data-date=""  class="input-append date dpYears">
                                <input type="text" value="12-10-2015" readonly size="16" id="txtdate" class="form-control">
                                <span class="input-group-btn add-on">
                                      <button class="btn btn-danger" type="button"><i class="icon-calendar" style="height: 15px;"></i></button>
                                </span>                                                  
                              </div>
                          </div>
                          <div class="col-md-5">
                          </div>         
                        </div> 
                      </div>
                      <div class="row">
                        <div class="col-md-12" >
                          <table class="table table-hover table-bordered" id="load_listtable">
                          </table>
                        </div>
                      </div>
                      <br>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="col-md-8">
                              <textarea  rows="6" class="form-control" id="ContentConvert"></textarea>
                          </div>
                          <div class="col-md-8">
                            <span id="sp_tbl" style="color:red;"></span>
                          </div>
                        </div>
                      </div>
                      <br>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="col-md-4"><input type="button" value="Convert all"  class="btn" id="btn_ConvertAll" onclick="ConvertAll();"></div>
                          <div class="col-md-4"><input type="button" value="Clear All"  class="btn btn-warning"  id="btn_ClearAll"   onclick="ClearAll();"></div>
                          <div class="col-md-4"><input type="button" value="Save"     class="btn btn-primary"    id="btn_Save"       onclick="SaveAll();"></div>
                        </div>
                      </div>
                    </div>
                       
                      </div>
                    </div>  
                    </div>
            </form>
                </div>
          </div>
          <!-- table end -->
        
          <!-- page end-->
      </section>
  </section>
  <!--main content end